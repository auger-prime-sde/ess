{
  "phase": "pretest",
  "tester": "Petr Tobiska",
  "datadir": "data-%Y%m%d/",
  "logging": {
    "level": 10,
    "format": "%(asctime)s | %(name)s | %(levelname)s | %(message)s",
    "filename": "pretest-%Y%m%d%H%M%S.log" },
  "comment": "Pretest",
  "lowgains": [ 1, 3, 5, 7, 8, 9 ],
  "highgains": [ 2, 4, 6, 10 ],
  "ports": {
    "power": "/dev/ttyACM0" },
  "tickers": {
    "essprogram": "pretestprog.json",
    "essprogram.macros" : { "chmac": "fw_setenv ethaddr $MACADDR" }},
  "power": {
    "ch1": [ 24, 0.5 ]},
  "RPiTrigger": true,
  "trigdelay": { "N": 0, "R": 0 },
  "dataloggers": {
    "ramp": true,
    "pede": true,
    "pickle": false,
    "db": { "dbuser": "sdeu-fzu",
	    "dbpass": "xxxxxxx",
	    "urlGetUUB": "https://auger-sdeu.farm.particle.cz/db/getuub/{uubnum}",
	    "urlCommit": "https://auger-sdeu.farm.particle.cz/db/commit",
	    "logitems": [ "noise" ]}},
  "uubnums": [ 3840, $UUBNUM ]
}
